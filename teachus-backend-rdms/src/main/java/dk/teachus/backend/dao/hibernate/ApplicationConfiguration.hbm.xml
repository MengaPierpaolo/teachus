<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="dk.teachus.backend.domain.impl" default-lazy="false">

    <class name="ApplicationConfigurationEntry" table="application_configuration">
        <id name="key" column="name" length="255" />
        
        <version name="version" access="field" />
        
        <property name="value" length="100000" />
    </class>

</hibernate-mapping>